{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Alif Ensemble Debug",
            "cwd": "${workspaceFolder}",
            "executable": "./out/cproject/${command:cpptools.activeConfigName}/cproject.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "showDevDebugOutput": "parsed",
            "device": "AE722F80F55D5_${command:cpptools.activeConfigName}",
            "interface": "jtag",
            "preLaunchCommands": [
                "monitor speed 15000"
            ],
            "jlinkscript": ".alif/m55-${command:cpptools.activeConfigName}_setup.jlinkscript"
        },

        {
            "name": "Alif Ensemble Multicore Debug",
            "cwd": "${workspaceFolder}",
            "executable": "./out/cproject/HP/cproject.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "showDevDebugOutput": "parsed",
            "device": "AE722F80F55D5_HP",
            "interface": "jtag",
            "preLaunchCommands": [
                "monitor speed 15000"
            ],
            //"jlinkscript": ".alif/m55-hp_setup.jlinkscript",  // Not needed with JLink v7.94b or higher.
            "chainedConfigurations": {
                "enabled": true,
                "waitOnEvent": "postInit",
                "detached": true,
                "lifecycleManagedByParent": true,
                "launches": [
                    {
                        "name": "(secondary) Alif Ensemble Multicore Debug",
                        "folder": "${workspaceFolder}"
                    }
                ]
            }
        },
        {
            "name": "(secondary) Alif Ensemble Multicore Debug",
            "cwd": "${workspaceFolder}",
            "executable": "./out/cproject/HE/cproject.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "showDevDebugOutput": "parsed",
            "device": "AE722F80F55D5_HE",
            "interface": "jtag",
            "presentation": {
                "hidden": true,
                "group": "",
                "order": 1
            },
            "preLaunchCommands": [
                "monitor speed 15000"
            ],
            //"jlinkscript": ".alif/m55-he_setup.jlinkscript"   // Not needed with JLink v7.94b or higher.
        }
    ]
}
